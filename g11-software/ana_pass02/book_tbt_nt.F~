      subroutine book_tbt_nt(ntid)
      IMPLICIT NONE
      integer ntid

#include "TBTnt.inc"

      call hbname(ntid,'TBTR',nTBTR,'nTBTR[0,100]:I,'//
     1'xTBTR(nTBTR):R,yTBTR(nTBTR):R,zTBTR(nTBTR):R,'//
     2'pxTBTR(nTBTR):R,pyTBTR(nTBTR):R,pzTBTR(nTBTR):R,'//
     3'qTBTR(nTBTR):R,c2TBTR(nTBTR):R,'//
     4'secTBTR(nTBTR):I,hbtTBTR(nTBTR)[1,20]:I')

      call hbname(ntid,'TBER',nTBER,'nTBER[0,100]:I,'//
     1'qpTBER(nTBER):R,lbTBER(nTBER):R,phiTBER(nTBER):R,'//
     2'd0TBER(nTBER):R,z0TBER(nTBER):R,c11TBER(nTBER):R,'//
     3'c12TBER(nTBER):R,c13TBER(nTBER):R,c14TBER(nTBER):R,c15TBER(nTBER):R,'//
     4'c22TBER(nTBER):R,c23TBER(nTBER):R,c24TBER(nTBER):R,c25TBER(nTBER):R,'//
     5'c33TBER(nTBER):R,c34TBER(nTBER):R,c35TBER(nTBER):R,'//
     6'c44TBER(nTBER):R,c45TBER(nTBER):R,'//
     7'c55TBER(nTBER):R,chi2TBER(nTBER):R,'//
     8'lay1TBER(nTBER):I,lay2TBER(nTBER):I')

      return
      end
